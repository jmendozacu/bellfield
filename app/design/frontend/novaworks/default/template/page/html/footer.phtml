<script>
    var myVideo = document.getElementById("video1");
    jQuery(".video-content").ready(function(){
        jQuery(".playvideo").click(function(){
            if (myVideo.paused){
                myVideo.play();
                jQuery(this).parent().parent().find('#video1').css('opacity','1');
                jQuery(this).html('<span class="icon-pause2"></span>');
            }
            else{
                myVideo.pause();
                jQuery(this).parent().parent().find('#video1').css('opacity','0');
                jQuery(this).html('<span class="icon-play2"></span>');
            }
        });

    });
    function endvideo() {
        jQuery(".video-content").ready(function(){
            jQuery(this).find('#video1').css('opacity','0');
            jQuery(".playvideo").html('<span class="icon-play2"></span>');
        });
    }
</script>

